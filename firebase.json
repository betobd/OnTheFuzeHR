{
  "hosting": {
    "redirects":[
      {
        "source": "/createEmployee",
        "destination": "createEmployee.html",
        "type": 301
      },
      {
        "source": "/inicio",
        "destination": "index.html",
        "type": 302
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "rewrites": [ {
    "source": "**",
    "destination": "/index.html"
  } ]
}
